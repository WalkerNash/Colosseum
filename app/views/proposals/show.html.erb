<%= render 'proposal', proposal: @proposal %>
<div class="grid-item-4">
  <h3>Project Resources:</h3>
  <ul>
    <li><%= link_to "Return to Proposal Index", proposals_path %></li>
    <% if @proposal.user.id == current_user.id %>
    <li><%= link_to "Edit Proposal", edit_proposal_path(@proposal) %></li>
    <li><%= link_to 'Destroy', @proposal, :method => :delete %></li>
    <% end %>
  </ul>
</div>
